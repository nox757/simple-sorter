<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet id="20190121-add-version-column-to-country-table" author="chibisov">
        <comment>Добавлена возможность хранения версии для страны</comment>
        <addColumn tableName="country">
            <column name="version" type="bigint" remarks="Версия"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>